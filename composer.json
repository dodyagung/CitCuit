{
    "name": "dodyagung/CitCuit",
    "description": "A mobile Twitter client alternative. Built with Laravel. https://citcuit.in",
    "keywords": ["citcuit", "mobile", "twitter", "client", "secure", "slim", "fast", "no ads", "no database", "laravel", "jakarta", "indonesia"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-debugbar": "^2.3",
        "htmlmin/htmlmin": "^5.0",
        "jublonet/codebird-php": "^3.1",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "sammyk/laravel-facebook-sdk": "^3.4",
        "sentry/sentry-laravel": "^0.7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
